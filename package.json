{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "description": "Starter for your Electron application",
  "version": "1.0.0",
  "author": "Alex Grozav <alex@pixevil.com>",
  "copyright": "Â© 2017, Pixevil inc.",
  "homepage": "http://pixevil.com",
  "license": "MIT",
  "main": "app/electron.js",
  "build": {
    "appId": "com.pixevil.electron-boilerplate",
    "files": [
      "application/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build -mwl",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha application/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha application/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "fs-jetpack": "^0.10.2"
  },
  "devDependencies": {
    "babili": "0.0.10",
    "chai": "^3.5.0",
    "coffee-script": "^1.12.3",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "electron-mocha": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-batch": "^1.0.5",
    "gulp-cached": "^1.1.1",
    "gulp-coffee": "^2.3.3",
    "gulp-cssmin": "^0.1.7",
    "gulp-debug": "^3.0.0",
    "gulp-ignore": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-include": "^2.3.1",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^2.0.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "nib": "^1.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  }
}
